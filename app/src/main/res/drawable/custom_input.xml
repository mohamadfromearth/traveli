            <solid android:color="?attr/colorBackground0" />
            <corners android:radius="4dp" />
            <stroke android:width="1dp" android:color="?attr/colorText0" />
        </shape>
    </item>

    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="?attr/colorBackground0" />
            <corners android:radius="4dp" />
